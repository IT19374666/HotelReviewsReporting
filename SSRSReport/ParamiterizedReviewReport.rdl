  <DataSets>
    <DataSet Name="DataSetReviews">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Hotel">
            <Value>=Parameters!Hotel.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DimDate.[Date]
  ,DimDate.[Month]
  ,DimDate.[Year]
  ,DimHotel.city
  ,DimHotel.country
  ,DimHotel.province
  ,DimHotel.name
  ,DimHotel.categories
  ,DimHotel.primaryCategories
  ,DimReviewer.reviews#userProvince
  ,DimReviewer.reviews#userCity
  ,FactReview.reviews#rating
  ,FactReview.[commision#$]
  ,FactReview.noOfSeens
FROM
  DimDate
  INNER JOIN FactReview
    ON DimDate.DateKey = FactReview.ReviewDateKey
  INNER JOIN DimHotel
    ON DimHotel.SK_hotel = FactReview.HotelKey
  INNER JOIN DimReviewer
    ON DimReviewer.SK_Reviewer = FactReview.ReviewerKey

where DimHotel.name in (@Hotel)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
